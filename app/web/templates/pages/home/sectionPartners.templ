package home

templ partners(phrases map[string]map[string]string) {
<section class="section donors-section">
	<div class="container">
		<div class="section-title animate-on-scroll">
			<h2>{ phrases["partners"]["title"] }</h2>
			<p>{ phrases["partners"]["subtitle"] }</p>
		</div>
		<div class="row g-4">
			<div class="col-lg-3 col-md-6">
				<div class="donor-card animate-on-scroll">
					<div class="donor-logo">
						<img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Flogos-download.com%2Fwp-content%2Fuploads%2F2016%2F03%2FLidl_logo.png&f=1&nofb=1&ipt=011675ed521f4c80cbb29b031fa3d7d10a0cabe56ae8e202734ca68f3a9d8072" alt={ phrases["partners"]["partner1_alt"] }>
					</div>
					<h4>{ phrases["partners"]["partner1_name"] }</h4>
					<p>{ phrases["partners"]["partner1_description"] }</p>
				</div>
			</div>
			<div class="col-lg-3 col-md-6">
				<div class="donor-card animate-on-scroll delay-1">
					<div class="donor-logo">
						<img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Flogodix.com%2Flogo%2F5031.jpg&f=1&nofb=1&ipt=7bcf7afc931fe4425a6feb444a2fd84c6a8e793aee6a8736c01a4d8a4b363ecb" alt={ phrases["partners"]["partner2_alt"] }>
