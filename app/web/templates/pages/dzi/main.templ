package dzi

import "NTG/web/templates/base"

templ components(phrases map[string]map[string]string) {
	<link rel="stylesheet" href="/static/css/dzi.css"/>
	@sectionHero(phrases)
	@sectionInfo(phrases)
	@sectionDocument(phrases)
	<script src="/static/js/dzi.js"></script>
}

templ Handler(page, language string, phrases map[string]map[string]string) {
	@base.Base(page, language, phrases, components(phrases))
}
