package base

templ navbar(page, language string, phrases map[string]map[string]string) {
	<nav class="navbar navbar-expand-lg navbar-dark">
		<div class="container">
			<div class="logo-container">
				<a href={ templ.URL("/" + language + "/home") }>
					<img src="/static/images/navbar/logo.png" alt={ phrases["navbar"]["ntg_logo_alt"] } class="logo"/>
				</a>
				<a href="https://www.ischools.bg/" target="_blank">
					<img src="/static/images/navbar/inovativno-uchilishte.jpg" alt={ phrases["navbar"]["innovative_logo_alt"] } class="innovative-logo"/>
				</a>
			</div>
			<div class="navbar-toggler-container">
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav">
					<span class="navbar-toggler-icon"></span>
				</button>
			</div>
			<div class="collapse navbar-collapse ms-2" id="navbarNav">
				<ul class="navbar-nav mx-auto">
					<li class="nav-item">
						if page == "home" {
							<a class="nav-link active" href={ templ.URL("/" + language + "/home") }>
								{ phrases["navbar"]["home"] }
							</a>
						} else {
							<a class="nav-link" href={ templ.URL("/" + language + "/home") }>
								{ phrases["navbar"]["home"] }
							</a>
						}
					</li>
					<li class="nav-item dropdown">
						if page == "about_us" {
							<a class="nav-link dropdown-toggle active" href={ templ.URL("/" + language + "/about_us") } id="aboutDropdown" role="button" data-bs-toggle="dropdown">
								{ phrases["navbar"]["about"] }
							</a>
						} else {
							<a class="nav-link dropdown-toggle" href={ templ.URL("/" + language + "/about_us") } id="aboutDropdown" role="button" data-bs-toggle="dropdown">
								{ phrases["navbar"]["about"] }
							</a>
						}
						<ul class="dropdown-menu">
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#history") }>{ phrases["navbar"]["history"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#leadership") }>{ phrases["navbar"]["leadership"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#qualifications") }>{ phrases["navbar"]["qualifications"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#values") }>{ phrases["navbar"]["values"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#public_council") }>{ phrases["navbar"]["public_council"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#school_board") }>{ phrases["navbar"]["school_board"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#school_council") }>{ phrases["navbar"]["school_council"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/about_us#cooperation") }>{ phrases["navbar"]["cooperation"] }</a></li>
						</ul>
					</li>
					<li class="nav-item">
						if page == "news" {
							<a class="nav-link active" href={ templ.URL("/" + language + "/news") }>
								{ phrases["navbar"]["news"] }
							</a>
						} else {
							<a class="nav-link" href={ templ.URL("/" + language + "/news") }>
								{ phrases["navbar"]["news"] }
							</a>
						}
					</li>
					// <li class="nav-item dropdown">
					// 	if page == "documents" {
					// 		<a class="nav-link dropdown-toggle active" href={ templ.URL("/" + language + "/documents") } id="docsDropdown" role="button" data-bs-toggle="dropdown">
					// 			{ phrases["navbar"]["documents"] }
					// 		</a>
					// 	} else {
					// 		<a class="nav-link dropdown-toggle" href={ templ.URL("/" + language + "/documents") } id="docsDropdown" role="button" data-bs-toggle="dropdown">
					// 			{ phrases["navbar"]["documents"] }
					// 		</a>
					// 	}
