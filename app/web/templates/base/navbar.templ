					// 	<li><a class="dropdown-item" href={ templ.URL("/" + language + "/documents") }></a></li>
					// @documentSectionsDropdown(language, phrases)
					// 
					<li class="nav-item">
						if page == "documents" {
							<a class="nav-link active" href={ templ.URL("/" + language + "/documents") }>
								{ phrases["navbar"]["documents"] }
							</a>
						} else {
							<a class="nav-link" href={ templ.URL("/" + language + "/documents") }>
								{ phrases["navbar"]["documents"] }
							</a>
						}
					</li>
					// </li>
					<li class="nav-item">
						if page == "admission" {
							<a class="nav-link active" href={ templ.URL("/" + language + "/admission") }>
								{ phrases["navbar"]["admission"] }
							</a>
						} else {
							<a class="nav-link" href={ templ.URL("/" + language + "/admission") }>
								{ phrases["navbar"]["admission"] }
							</a>
						}
					</li>
					<li class="nav-item dropdown">
						if page == "programs_and_projects" {
