								{ phrases["navbar"]["programs_and_projects"] }
							</a>
						} else {
							<a class="nav-link dropdown-toggle" href={ templ.URL("/" + language + "/programs_and_projects") } id="programsDropdown" role="button" data-bs-toggle="dropdown">
								{ phrases["navbar"]["programs_and_projects"] }
							</a>
						}
						<ul class="dropdown-menu">
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/programs_and_projects#erazmus-projects-section-title") }>{ phrases["navbar"]["erazmus"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/programs_and_projects#projects-section-title") }>{ phrases["navbar"]["national-programs"] }</a></li>
						</ul>
					</li>
					<li class="nav-item dropdown">
						if page == "school_life" || page == "education" || page == "professional_education" || page == "general_education" || page == "interest_activities" || page == "e_diary" || page == "uniforms" || page == "document_templates" || page == "scholarships" || page == "nvo" || page == "dzi" || page == "zdippk" || page == "academic_schedule" {
							<a class="nav-link dropdown-toggle active" href={ templ.URL("/" + language + "/school_life") } id="schoolLifeDropdown" role="button" data-bs-toggle="dropdown">
								{ phrases["navbar"]["school_life"] }
							</a>
						} else {
							<a class="nav-link dropdown-toggle" href={ templ.URL("/" + language + "/school_life") } id="schoolLifeDropdown" role="button" data-bs-toggle="dropdown">
								{ phrases["navbar"]["school_life"] }
							</a>
						}
						<ul class="dropdown-menu">
							<li class="dropdown-submenu">
								<span class="dropdown-item dropdown-toggle">{ phrases["navbar"]["education"] }</span>
								<ul class="dropdown-menu">
									<li><a class="dropdown-item" href={ templ.URL("/" + language + "/professional_education") }>{ phrases["navbar"]["professional_education"] }</a></li>
									<li><a class="dropdown-item" href={ templ.URL("/" + language + "/general_education") }>{ phrases["navbar"]["general_education"] }</a></li>
									// <li><a class="dropdown-item disabled" href={ templ.URL("/" + language + "/interest_activities") }>{ phrases["navbar"]["interest_activities"] }</a></li>
									<li><a class="dropdown-item" href="https://www.shkolo.bg/" target="_blank" rel="noopener noreferrer">{ phrases["navbar"]["e_diary"] }</a></li>
								</ul>
							</li>
							// Disables as uniforms are not yet available
							// <li><a class="dropdown-item disabled" href={ templ.URL("/" + language + "/uniforms") }>{ phrases["navbar"]["uniforms"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/document_templates") }>{ phrases["navbar"]["document_templates"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/scholarships") }>{ phrases["navbar"]["scholarships"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/specs") }>{ phrases["navbar"]["specs"] }</a></li>
							<li><a class="dropdown-item uppercase" href={ templ.URL("/" + language + "/sfo") }>{ phrases["navbar"]["sfo"] }</a></li>
							<li><a class="dropdown-item uppercase" href={ templ.URL("/" + language + "/nvo") }>{ phrases["navbar"]["nvo"] }</a></li>
							<li><a class="dropdown-item uppercase" href={ templ.URL("/" + language + "/dzi") }>{ phrases["navbar"]["dzi"] }</a></li>
							<li><a class="dropdown-item uppercase" href={ templ.URL("/" + language + "/zdippk") }>{ phrases["navbar"]["zdippk"] }</a></li>
							<li><a class="dropdown-item" href={ templ.URL("/" + language + "/academic_schedule") }>{ phrases["navbar"]["academic_schedule"] }</a></li>
						</ul>
					</li>
					<li class="nav-item">
						if page == "partners" {
							<a class="nav-link active" href={ templ.URL("/" + language + "/partners") }>
								{ phrases["navbar"]["partners"] }
							</a>
						} else {
							<a class="nav-link" href={ templ.URL("/" + language + "/partners") }>
								{ phrases["navbar"]["partners"] }
							</a>
						}
					</li>
					<li class="nav-item">
						if page == "contacts" {
							<a class="nav-link active" href={ templ.URL("/" + language + "/contacts") }>
								{ phrases["navbar"]["contacts"] }
							</a>
						} else {
							<a class="nav-link" href={ templ.URL("/" + language + "/contacts") }>
								{ phrases["navbar"]["contacts"] }
							</a>
						}
					</li>
					<li class="nav-item dropdown">
						<a class="nav-link dropdown-toggle" href="#" id="langDropdown" role="button" data-bs-toggle="dropdown">
							if language == "bg" {
								<img src="/static/images/navbar/bg.svg" alt="Bulgarian Flag" class="flag-icon"/> Български
							} else if language == "en" {
								<img src="/static/images/navbar/en.svg" alt="British Flag" class="flag-icon"/> English
							} else if language == "de" {
								<img src="/static/images/navbar/de.svg" alt="German Flag" class="flag-icon"/> Deutsch
							} else if language == "fr" {
								<img src="/static/images/navbar/fr.svg" alt="French Flag" class="flag-icon"/> Français
							} else if language == "es" {
								<img src="/static/images/navbar/es.svg" alt="French Flag" class="flag-icon"/> Español
							}
						</a>
						<ul class="dropdown-menu">
							<li>
								<a class="dropdown-item" href={ templ.URL("/bg/" + page) }>
									<img src="/static/images/navbar/bg.svg" alt="Bulgarian Flag" class="flag-icon"/> Български
								</a>
							</li>
							<li>
								<a class="dropdown-item" href={ templ.URL("/en/" + page) }>
									<img src="/static/images/navbar/en.svg" alt="British Flag" class="flag-icon"/> English
								</a>
							</li>
							<li>
								<a class="dropdown-item" href={ templ.URL("/de/" + page) }>
									<img src="/static/images/navbar/de.svg" alt="German Flag" class="flag-icon"/> Deutsch
								</a>
							</li>
							<li>
								<a class="dropdown-item" href={ templ.URL("/fr/" + page) }>
									<img src="/static/images/navbar/fr.svg" alt="French Flag" class="flag-icon"/> Français
								</a>
							</li>
							<li>
								<a class="dropdown-item" href={ templ.URL("/es/" + page) }>
									<img src="/static/images/navbar/es.svg" alt="Spanish Flag" class="flag-icon"/> Español
								</a>
							</li>
						</ul>
					</li>
				</ul>
			</div>
		</div>
	</nav>
